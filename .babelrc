{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source",'transform-decorators-legacy',
    [
      "module-resolver",
      {
        "extensions": [".js", ".ios.js", ".android.js"],
        "cwd": "babelrc",
        "alias": {
          'SRC': './src',
          'IMAGES': './src/images',
          'THEMES': './src/themes',
          'CONTAINERS': './src/containers',
          'COMPONENTS': './src/components',
          'NAVIGATIONS': './src/navigations',
          'CONSTANTS': './src/constants',
          'SERVICE': './src/service',
          'UTILS': './src/utils',
          'REDUX': './src/redux',
          'SAGAS': './src/redux/sagas',
          'ACTIONS': './src/redux/actions',
        }
      }
    ]]
    },

    "test": {
      "plugins": ["transform-react-jsx-source",'transform-decorators-legacy',
    [
      "module-resolver",
      {
        "extensions": [".js", ".ios.js", ".android.js"],
        "cwd": "babelrc",
        "alias": {
          'SRC': './src',
          'IMAGES': './src/images',
          'THEMES': './src/themes',
          'CONTAINERS': './src/containers',
          'COMPONENTS': './src/components',
          'NAVIGATIONS': './src/navigations',
          'CONSTANTS': './src/constants',
          'SERVICE': './src/service',
          'UTILS': './src/utils',
          'REDUX': './src/redux',
          'SAGAS': './src/redux/sagas',
          'ACTIONS': './src/redux/actions',
        }
      }
    ]]
    },
    "production": {
      "plugins": ["transform-remove-console", "transform-react-remove-prop-types",'transform-decorators-legacy',
      [
        "module-resolver",
        {
          "root": ["./src"],
          "alias": {
            'SRC': './src',
            'IMAGES': './src/images',
            'THEMES': './src/themes',
            'CONTAINERS': './src/containers',
            'COMPONENTS': './src/components',
            'NAVIGATIONS': './src/navigations',
            'CONSTANTS': './src/constants',
            'SERVICE': './src/service',
            'UTILS': './src/utils',
            'REDUX': './src/redux',
            'SAGAS': './src/redux/sagas',
            'ACTIONS': './src/redux/actions',
          }
        }
      ]]
    }
  }
}
